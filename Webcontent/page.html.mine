				<div id="encoderEtudiant">

					<div id="page-wrapper" class="col-lg-8 md-6">
						<h2 class="col-sm-offset-4">Encoder Données étudiant</h2>
						<form method="post" class="form-horizontal" role="form"
							id="formEtudiant">

							<div class="form-group">
								<label class="control-label col-sm-3" for="text">Nom : </label>
								<div class="col-sm-7">
									<input type="text" id="nomId" name="nom" class="form-control">
								</div>
							</div>
							<div class="form-group">
								<label class="control-label col-sm-3" for="text">Prenom
									: </label>
								<div class="col-sm-7">
									<input type="text" id="prenomId" name="prenom"
										class="form-control">
								</div>
							</div>
							<div class="form-group">
								<label class="control-label col-sm-3" for="date">Date de
									naissance : </label>
								<div class="col-sm-7">
									<input type="date" id="dateId" name="date" class="form-control">
								</div>
							</div>
							<div class="form-group">
								<label class="control-label col-sm-3" for="text">Nationalité
									: </label>
								<div class="col-sm-7">
									<input type="text" id="natId" name="nationalite"
										class="form-control">
								</div>
							</div>
							<div class="form-group">
								<label class="control-label col-sm-3" for="text">Rue : </label>
								<div class="col-sm-7">
									<input type="text" id="rueeId" name="rue" class="form-control">
								</div>
							</div>
							<div class="form-group">
								<label class="control-label col-sm-3" for="number">N° :
								</label>
								<div class="col-sm-7">
									<input type="number" id="numeId" name="num" class="form-control">
								</div>
							</div>
							<div class="form-group">
								<label class="control-label col-sm-3" for="text">Boîte :
								</label>
								<div class="col-sm-7">
									<input type="text" id="boiteeId" name="boite"
										class="form-control" value="/">
								</div>
							</div>
							<div class="form-group">
								<label class="control-label col-sm-3" for="text">Region
									: </label>
								<div class="col-sm-7">
									<input type="text" id="regioneId" name="region"
										class="form-control">
								</div>
							</div>
							<div class="form-group">
								<label class="control-label col-sm-3" for="text">Code
									Postal: </label>
								<div class="col-sm-7">
									<input type="text" id="cpeId" name="cp"
										class="form-control">
								</div>
							</div>
							<div class="form-group">
								<label class="control-label col-sm-3" for="text">Ville :
								</label>
								<div class="col-sm-7">
									<input type="text" id="villeeId" name="ville"
										class="form-control">
								</div>
							</div>
							<div class="form-group">
								<label class="control-label col-sm-3" for="email">Email
									: </label>
								<div class="col-sm-7">
									<input type="text" id="emaileId" name="email"
										class="form-control">
								</div>
							</div>
							<div class="form-group">
								<label class="control-label col-sm-3" for="number">Nombre
									d'années réussies : </label>

								<div class="col-sm-7">
									<input type="number" min="0" max="2" id="nbrAnneeSuperieurId"
										name="nbrAnneeSuperieur" class="form-control">
								</div>
							</div>
							<div class="form-group">
								<label class="control-label col-sm-3" for="radio">Sexe :
								</label>
								<div class="row col-sm-7">
									H <input type="radio" name="sexe" value="M"> F <input
										type="radio" name="sexe" value="F">
								</div>
							</div>
							<div class="form-group">
								<label class="control-label col-sm-3" for="text">Compte
									IBAN:</label>
								<div class="col-sm-7">
									<input type="text" class="form-control" id="numCompteId"
										name="numCompte">
										<!-- pattern="^BE\d{14}$}#$" -->
								</div>
							</div>
							<div class="form-group">
								<label class="control-label col-sm-3" for="text">Titulaire:</label>
								<div class="col-sm-7">
									<input type="text" class="form-control" name="titulaire"
										id="titulaireId" placeholder="facultatif">
								</div>
							</div>
							<div class="form-group">
								<label class="control-label col-sm-3" for="password">Code
									Bic:</label>
								<div class="col-sm-7">
									<input type="text" class="form-control" name="bic" id="bicId">
								</div>
							</div>
							<div class="col-sm-offset-9">
								<button type="submit" class="btn btn-default" id="envoiEtud">Valider</button>
							</div>
						</form>
						<section class="notification"></section>
					</div>

				</div>
				<div id="soumettreDemande">
					<div id="page-wrapper" class="col-lg-8 md-6">
						<h2 class="col-sm-offset-4" id="titre">Soumettre une demande</h2>
						<form method="post" class="form-horizontal" role="form"
							id="formDemandeMobilite">
							<div class="form-group">
								<label class="control-label col-sm-3" for="text">Partenaire:
								</label>
								<div class="col-sm-7">
									<select class="form-control col-sm-3" id="partenaireById"
										name="nomPartenaire">
										<option value="newPart" id="newPartenaireOption">Nouveau
											partenaire ?</option>
									</select>
								</div>
							</div>
							<div class="form-group">
								<label class="control-label col-sm-3" for="text">Type
									mobilité: </label>
								<div class="col-sm-4">
									<select class="form-control col-sm-3" id="typeMobId"
										name="typeMobilite">
										<option>ERASMUS</option>
										<option>FAME</option>
										<option>ERABEL</option>
									</select>
								</div>
							</div>
							<div class="form-group">
								<label class="control-label col-sm-3" for="text">Priorité:
								</label>
								<div class="col-sm-7">
									<input type="number" id="prioriteId" name="ordrePreference"
										class="form-control">
								</div>
							</div>
							<div class="form-group">
								<label class="control-label col-sm-3" for="text">Type: </label>
								<div class="col-sm-4">
									<select class="form-control col-sm-3" id="typeSejourId"
										name="typeDemande">
										<option>SMS</option>
										<option>SMP</option>
									</select>
								</div>
							</div>
							<div class="form-group">
								<label class="control-label col-sm-3" for="text">Année:
								</label>
								<div class="col-sm-4">
									<select class="form-control col-sm-3" id="anneeId"
										name="anneeAcademique">

									</select>
								</div>
							</div>
							<div class="form-group">
								<label class="control-label col-sm-3" for="text">Periode:
								</label>
								<div class="col-sm-4">
									<select class="form-control col-sm-3" id="periodeId"
										name="periode">
										<option>Q1</option>
										<option>Q2</option>
									</select>
								</div>
							</div>
							<div class="col-sm-offset-8">
								<button type="submit" class="btn btn-default" id="send_demande" class="send">Valider</button>
							</div>
						</form>
						<section class="notification"></section>
					</div>
				</div>

